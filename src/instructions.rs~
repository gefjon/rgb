#![allow(non_snake_case)]
enum DecodedInstruction {
    // src: http://pastraiser.com/cpu/gameboy/gameboy_opcodes.html
    NOP = 0x00,
    LD_BC_d16 = 0x01,
    LD_BC_A = 0x02,
    INC_BC = 0x03,
    INC_B = 0x04,
    DEC_B = 0x05,
    LD_B_d8 = 0x06,
    RLCA = 0x07,
    LD_a16_SP = 0x08,
    ADD_HL_BC = 0x09,
    LD_A_BC = 0x0a,
}
